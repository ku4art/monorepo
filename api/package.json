{
  "name": "@repo/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch ./src/* ./src/index.ts",
    "build": "rimraf ./dist && tsc --build tsconfig.json",
    "start": "node ./dist/index.js",
    "check-ts": "tsc --noEmit",
    "gen:index": "vscode-generate-index-standalone src/ scripts/",
    "lint:fix": "eslint . --fix",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "6.4.1",
    "@trpc/server": "11.8.0",
    "cors": "2.8.5",
    "date-fns": "^4.1.0",
    "express": "5.2.1",
    "lodash": "^4.17.21",
    "prisma": "6.4.1",
    "superjson": "^2.2.6",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.19",
    "@types/express": "5.0.6",
    "@types/lodash": "^4.17.21",
    "rimraf": "6.1.2",
    "ts-node": "10.9.2",
    "tsx": "4.21.0"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  }
}
